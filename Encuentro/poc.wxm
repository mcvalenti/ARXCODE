/* [wxMaxima: input   start ] */
r_tca:matrix([7.72399682e-01],[-1.38777878e-17]);
/* [wxMaxima: input   end   ] */

/* [wxMaxima: input   start ] */
C_b:matrix([ 3.64942069,-0.08802068],[-0.08802068,0.02698636]);
